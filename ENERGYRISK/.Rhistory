randPolarRejc <- function(){
x2_available = FALSE
x1=0;x2=0;u1=0;u2=0;w=0;c=0
if(x2_available){
x2_available = FALSE
randPolarRejc = x2
}else{
repeat{
u1 = runif(1) * 2 - 1
u2 = runif(1) * 2 - 1
w = u1 * u1 + u2 * u2
if(w <= 1){
c = sqrt(-2 * log(w) / w)
x1 = c * u1
x2 = c * u2
x2_available <<- TRUE
randPolarRejc = x1
break
}
}
}
return(randPolarRejc)
}
